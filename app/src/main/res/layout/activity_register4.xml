<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/register3"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/odyssiaregisterpage"
    tools:context=".Register">

    <ImageView
        android:id="@+id/odyssiaRegisterText"
        android:layout_width="334dp"
        android:layout_height="177dp"
        android:layout_marginTop="412dp"
        android:background="@drawable/odyssiaregistertext"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_margin="7dp"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/buttonNext"
        android:layout_width="168dp"
        android:layout_height="49dp"
        android:background="@drawable/buttoncontinue"
        android:contentDescription="boton continuar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/odyssiaRegisterText"
        app:layout_constraintVertical_bias="0.677" />


</androidx.constraintlayout.widget.ConstraintLayout>